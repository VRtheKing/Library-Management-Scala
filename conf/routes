# Routes

# User routes
POST    /users                     controllers.UserController.createUser
GET     /users                     controllers.UserController.listUsers
GET     /borrowedBooks/:userId     controllers.UserController.borrowed

# Book routes
POST    /books                     controllers.BookController.createBook
GET     /books                     controllers.BookController.listBooks

# Checkout routes
POST    /checkouts                 controllers.CheckoutController.createCheckout
POST    /checkouts/:checkoutId/return      controllers.CheckoutController.returnBook(checkoutId: Long)

# Notification Log route
GET     /notifications        controllers.NotificationController.getNotifications